{
    "log": {
        "loglevel": "warning",
        "access": "/var/lib/xray/access.log",
        "error": "/var/lib/xray/error.log"
    },
    "inbounds": [
        {
            "port": 443,
            "protocol": "vless",
            "settings": {
                "clients": [],
                "decryption": "none"
            },
            "streamSettings": {
                "network": "tcp",
                "security": "reality",
                "realitySettings": {
                    "show": false,
                    "xver": 0,
                    "dest": "github.com:443",
                    "serverNames": [
                        "github.com",
                        "www.github.com",
                        "raw.githubusercontent.com",
                        "gist.github.com"
                    ],
                    "privateKey": "YOUR_PRIVATE_KEY",
                    "shortIds": ["YOUR_SHORT_ID"]
                }
            }
        },
        {
            "port": 8080,
            "protocol": "vless",
            "settings": {
                "clients": [],
                "decryption": "none"
            },
            "streamSettings": {
                "network": "xhttp",
                "security": "tls",
                "xhttpSettings": {
                    "host": "${DOMAIN}",
                    "path": "/xhttp",
                    "method": "POST",
                    "headers": {
                        "Content-Type": ["application/octet-stream", "application/x-www-form-urlencoded"]
                    }
                },
                "tlsSettings": {
                    "alpn": ["h2", "http/1.1"]
                }
            }
        },
        {
            "port": 2087,
            "protocol": "vless",
            "settings": {
                "clients": [],
                "decryption": "none"
            },
            "streamSettings": {
                "network": "xhttp",
                "security": "tls",
                "xhttpSettings": {
                    "host": "${DOMAIN}",
                    "path": "/xhttp/stream",
                    "method": "POST",
                    "headers": {
                        "Content-Type": ["application/octet-stream", "application/x-www-form-urlencoded"]
                    }
                },
                "tlsSettings": {
                    "alpn": ["h2", "http/1.1"]
                }
            }
        }
    ],
    "outbounds": [
        {
            "protocol": "freedom",
            "settings": {}
        }
    ]
} 